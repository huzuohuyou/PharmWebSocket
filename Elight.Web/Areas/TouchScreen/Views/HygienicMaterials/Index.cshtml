<!--内容模板-->

<script type="text/javascript">

    var paging;
    layui.config({
        base: parent._baseUrl
    }).use(['paging', 'form', 'layer'], function () {
        var layer = parent.layer || layui.layer;
        var form = layui.form();
        paging = layui.paging();
        $('#btn_jczl').click(btn_clfl);
        $('#btn_pycx').click(btn_pycx);
    });

    function btn_clfl() {
        $.layerOpen({
            id: "edit",
            title: "材料分类",
            width: "90%",
            height: "100%",
            content: "/TouchScreen/HygienicMaterials/HygienicMaterials",
            btn: []
        });
    }

    function btn_pycx() {
        $.layerOpen({
            id: "edit",
            title: "拼音",
            width: "90%",
            height: "100%",
            content: "/TouchScreen/PinYin/Material2",
            btn: []
        });
    }
    
</script>
